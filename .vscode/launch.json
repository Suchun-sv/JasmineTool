{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "JasmineTool: Debug start command",
            "type": "python",
            "request": "launch",
            "module": "src.jasminetool.cli",
            "args": [
                "-t",
                "test_ubuntu",
                "start"
            ],
            "cwd": "${workspaceFolder}",
            "console": "integratedTerminal",
